.
catenaxAddition:hasPostConsumerRecycledMaterialShare a owl:ObjectProperty ;
    rdfs:comment "Recycled material share from post-consumer waste (end-of-life scrap) of the active material." ;
    rdfs:label "Recycled Material Share from post-Consumer Waste" ;
    rdfs:domain catenax:BatteryPass ;
    rdfs:range catenax:PostConsumerRecycledMaterialShare
.
catenaxAddition:PostConsumerRecycledMaterialShare a owl:Class ; 
    rdfs:comment "Recycled material share from post-consumer waste (end-of-life scrap) of the active material." ;
    rdfs:label "Recycled Material Share from post-Consumer Waste" ;
    rdfs:subClassOf catenaxAddition:RenewableContentShare   
.
catenaxAddition:recycledMaterialShareName a owl:DatatypeProperty ;
    rdfs:comment "Recycled material name from post/pre-consumer waste of the active material." ;
    rdfs:label "Recycled Material Share Name" ;
    rdfs:domain catenaxAddition:RenewableContentShare ;
    rdfs:range xsd:string
.
catenaxAddition:recycledMaterialShareAmount a owl:DatatypeProperty ;
    rdfs:comment "Recycled material share amount from post/pre-consumer waste of the active material." ;
    rdfs:label "Recycled Material Share Amount (percent)" ;
    rdfs:domain catenaxAddition:RenewableContentShare ;
