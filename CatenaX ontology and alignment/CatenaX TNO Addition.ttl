    rdfs:label "Recycled material name and share from post/pre-consumer waste of the active material." ;
    rdfs:domain catenax:BatteryPass ;
    rdfs:range catenax:RenewableContentShare   
.
catenaxAddition:RenewableContentShare a owl:Class ;
    rdfs:comment "Recycled material name and share from post/pre-consumer waste of the active material." ;
    rdfs:label "Recycled material name and share from post/pre-consumer waste of the active material." ;   
.
catenaxAddition:hasPreConsumerRecycledMaterialShare a owl:ObjectProperty ;
